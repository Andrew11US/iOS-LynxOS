  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'pop', '~> 1.0'

  target 'LynxOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LynxOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
